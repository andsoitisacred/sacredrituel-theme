    "85bb7a87-2593-464c-9ced-f001de178bad": {
      "type": "images-with-text-slider",
      "blocks": {
        "template--14835198820406__85bb7a87-2593-464c-9ced-f001de178bad-1667422543a71f72fd-0": {
          "type": "image",
          "settings": {
            "image": "shopify://shop_images/howtoapply-129193.jpg",
            "heading": "1. Spray Rose Toner",
            "description": "<p>Starting with a clean, dry face, generously spray our Sacred Rose Mist to your face and neck.</p>"
