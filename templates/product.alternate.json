        },
        "template--14835198820406__85bb7a87-2593-464c-9ced-f001de178bad-1667422543a71f72fd-1": {
          "type": "image",
          "settings": {
            "image": "shopify://shop_images/howtoapply-129321.jpg",
            "heading": "2. Apply the Serum",
            "description": "<p>Using the dropper vial, apply Sacred Serum to your open palm or directly to your skin.</p>"
          }
        },
        "template--14835198820406__85bb7a87-2593-464c-9ced-f001de178bad-1667422543a71f72fd-2": {
          "type": "image",
